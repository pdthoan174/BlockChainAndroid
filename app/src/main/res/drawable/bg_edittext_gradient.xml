<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="45"
                android:endColor="@color/cyan"
                android:centerColor="@color/teal"
                android:startColor="@color/green" />
            <corners android:radius="@dimen/_10sdp" />
            <padding
                android:bottom="@dimen/_1sdp"
                android:left="@dimen/_1sdp"
                android:right="@dimen/_1sdp"
                android:top="@dimen/_1sdp" />
            <size
                android:width="@dimen/_162sdp"
                android:height="@dimen/_44sdp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_navigation" />
            <corners android:radius="@dimen/_10sdp" />
            <size
                android:width="@dimen/_50sdp"
                android:height="@dimen/_50sdp" />
        </shape>
    </item>
</layer-list>